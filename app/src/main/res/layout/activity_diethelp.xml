<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="diethelpVM" type="com.bedirhansapplication.app.modules.diethelp.data.viewmodel.DiethelpVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAndroidSmall"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleblue_200">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDietHelp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_158pxh"
                        android:layout_marginStart="@dimen/_117pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{diethelpVM.diethelpModel.txtDietHelp}"
                        style="@style/txtPoppinssemibold20" tools:text="@string/lbl_diet_help"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowuser"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_69pxh"
                    android:layout_width="@dimen/_54pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWhatkindofdi"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{diethelpVM.diethelpModel.txtWhatkindofdi}"
                    style="@style/txtPoppinssemibold17" tools:text="@string/msg_what_kind_of_di"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtMaintenanceCal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{diethelpVM.diethelpModel.txtMaintenanceCal}"
                style="@style/txtPoppinssemibold17" tools:text="@string/msg_maintenance_cal"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnfemale"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_4">
                <FrameLayout android:id="@+id/frameStackfemale"
                    android:layout_height="@dimen/_44pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_185pxh">
                    <RadioButton android:buttonTint="@color/white_A700"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:id="@+id/rbFemale"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_2pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_female"
                        android:textColor="@color/gray_900"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="normal" tools:text="@string/lbl_female"/>
                    <FrameLayout android:id="@+id/frameStackellipsenine"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_41pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_126pxh">
                        <LinearLayout
                            android:id="@+id/linearColumnellipsenine"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content"
                            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                            <View
                                android:background="@drawable/rectangle_bg_bluegray_900_radius_6_28"
                                android:id="@+id/viewEllipseNine"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginHorizontal="@dimen/_4pxh"
                                android:layout_marginVertical="@dimen/_4pxv" android:layout_width="@dimen/_12pxh"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGender"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_31pxh"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{diethelpVM.diethelpModel.txtGender}"
                            style="@style/txtPoppinsregular14" tools:text="@string/lbl_male"/>
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtChooseyourgen"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{diethelpVM.diethelpModel.txtChooseyourgen}"
                            style="@style/txtPoppinsbold12" tools:text="@string/msg_choose_your_gen"/>
                    </FrameLayout>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChooseyourlif"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{diethelpVM.diethelpModel.txtChooseyourlif}"
                    style="@style/txtPoppinsbold12" tools:text="@string/msg_choose_your_lif"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowellipsetwelve"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnellipsetwelve"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_weight="0.08"
                        android:layout_width="0dp" android:orientation="vertical">
                        <View
                            android:background="@drawable/rectangle_bg_white_a700_radius_10_845"
                            android:id="@+id/viewEllipseTwelve"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_21pxh" android:layout_width="@dimen/_21pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_white_a700_radius_10_845"
                            android:id="@+id/viewEllipseTen"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginTop="@dimen/_6pxv" android:layout_width="@dimen/_21pxh"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnellipsefifteen"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                            <View
                                android:background="@drawable/rectangle_bg_bluegray_900_radius_6_28"
                                android:id="@+id/viewEllipseFifteen"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginHorizontal="@dimen/_4pxh"
                                android:layout_marginVertical="@dimen/_4pxv" android:layout_width="@dimen/_12pxh"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnsedentary"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.92"
                        android:layout_width="0dp" android:orientation="vertical">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowsedentary"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSedentary"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.41"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{diethelpVM.diethelpModel.txtSedentary}"
                                style="@style/txtPoppinsregular14" tools:text="@string/lbl_sedentary"/>
                            <RadioButton
                                android:buttonTint="@color/white_A700"
                                android:ellipsize="end"
                                android:fontFamily="@font/poppins"
                                android:id="@+id/rbVeryActive"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_50pxh"
                                android:layout_weight="0.59"
                                android:layout_width="0dp"
                                android:paddingStart="@dimen/_6pxh"
                                android:paddingTop="@dimen/_1pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_very_active"
                                android:textColor="@color/gray_900"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_14pxh"
                                android:textStyle="normal" tools:text="@string/lbl_very_active"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRowlightlyactive"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLightlyActive"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.44"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{diethelpVM.diethelpModel.txtLightlyActive}"
                                style="@style/txtPoppinsregular14" tools:text="@string/lbl_lightly_active"/>
                            <RadioButton
                                android:buttonTint="@color/white_A700"
                                android:ellipsize="end"
                                android:fontFamily="@font/poppins"
                                android:id="@+id/rbSuperActive"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_30pxh"
                                android:layout_weight="0.56"
                                android:layout_width="0dp"
                                android:paddingStart="@dimen/_6pxh"
                                android:paddingTop="@dimen/_1pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_super_active"
                                android:textColor="@color/gray_900"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_14pxh"
                                android:textStyle="normal" tools:text="@string/lbl_super_active"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtModeratelyActi"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{diethelpVM.diethelpModel.txtModeratelyActi}"
                            style="@style/txtPoppinsregular14" tools:text="@string/msg_moderately_acti"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowlanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.77"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{diethelpVM.diethelpModel.txtLanguage}"
                        style="@style/txtPoppinsbold12" tools:text="@string/msg_enter_your_weig"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWeight"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.23"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{diethelpVM.diethelpModel.txtWeight}"
                        style="@style/txtPoppinsbold12_1" tools:text="@string/lbl_80_kg"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDietPlanner"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{diethelpVM.diethelpModel.txtDietPlanner}"
                style="@style/txtPoppinssemibold17" tools:text="@string/lbl_diet_planner"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumngroupfiftynine"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_4">
                <EditText
                    android:background="@drawable/rectangle_bg_blue_201_radius_6_04"
                    android:ellipsize="end"
                    android:hint="@string/msg_protein_based_d"
                    android:id="@+id/etGroupFiftyNine"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_5pxv"
                    android:singleLine="true"
                    android:text="@={diethelpVM.diethelpModel.etGroupFiftyNineValue}"
                    android:textColorHint="@color/gray_900"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_6_04"
                    android:ellipsize="end"
                    android:hint="@string/lbl_ketogenic_diet"
                    android:id="@+id/etGroupFiftyEight"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@={diethelpVM.diethelpModel.etGroupFiftyEightValue}"
                    android:textColorHint="@color/gray_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_6_04"
                    android:ellipsize="end"
                    android:hint="@string/msg_weight_loss_die"
                    android:id="@+id/etGroupFiftySeven"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@={diethelpVM.diethelpModel.etGroupFiftySevenValue}"
                    android:textColorHint="@color/gray_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_6_04"
                    android:ellipsize="end"
                    android:hint="@string/msg_weight_gain_die"
                    android:id="@+id/etGroupFiftySix"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@={diethelpVM.diethelpModel.etGroupFiftySixValue}"
                    android:textColorHint="@color/gray_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnGenerate"
                android:layout_gravity="end"
                android:layout_height="@dimen/_43pxv"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_marginTop="@dimen/_43pxv"
                android:layout_width="@dimen/_108pxh"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_14pxh"
                android:paddingStart="@dimen/_13pxh"
                android:paddingTop="@dimen/_8pxv"
                android:singleLine="true"
                android:text="@string/lbl_generate"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_generate"/>
        </LinearLayout>
    </FrameLayout>
</layout>
