<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="addproductVM" type="com.bedirhansapplication.app.modules.addproduct.data.viewmodel.AddproductVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAndroidSmall"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleblue_200">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_25pxv"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAddProduct"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_139pxh"
                        android:layout_marginStart="@dimen/_96pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{addproductVM.addproductModel.txtAddProduct}"
                        style="@style/txtPoppinssemibold20" tools:text="@string/lbl_add_product"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:id="@+id/linearColumnenterproductn"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_1">
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtEnterproductn"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{addproductVM.addproductModel.txtEnterproductn}"
                    style="@style/txtPoppinsregular19_1" tools:text="@string/msg_enter_product_n"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtGroupEightySeven"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_6pxh"
                    android:paddingTop="@dimen/_1pxv"
                    android:singleLine="true"
                    android:text="@{addproductVM.addproductModel.txtGroupEightySeven}"
                    style="@style/txtSolidRounded" tools:text="@string/lbl_product_x2"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtGroupEightySix"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_41pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_6pxh"
                    android:paddingTop="@dimen/_2pxv"
                    android:singleLine="true"
                    android:text="@{addproductVM.addproductModel.txtGroupEightySix}"
                    style="@style/txtSolidRounded" tools:text="@string/lbl_11_12_20232"/>
            </LinearLayout>
            <EditText
                android:background="@drawable/rectangle_bg_blue_200_radius_13_64"
                android:drawableEnd="@drawable/img_camera"
                android:drawablePadding="@dimen/_19pxh"
                android:ellipsize="end"
                android:hint="@string/lbl_add_image"
                android:id="@+id/etGroupNinety" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_8pxh"
                android:paddingStart="@dimen/_9pxh"
                android:paddingTop="@dimen/_12pxv"
                android:singleLine="true"
                android:text="@={addproductVM.addproductModel.etGroupNinetyValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRounded_4" tools:ignore="Autofill"/>
            <ImageView android:id="@+id/imagePhotoAddProd"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_234pxh"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_238pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_5pxh}"
                imageRes="@{@drawable/img_resim2023103_52x56}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_resim2023103_52x56"/>
            <LinearLayout android:id="@+id/linearRowgroupeightyfive"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_60pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowaddingredients"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.52"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyleblue_200cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAddIngredients"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{addproductVM.addproductModel.txtAddIngredients}"
                        style="@style/txtPoppinssemibold17" tools:text="@string/lbl_add_ingredients"/>
                    <ImageView android:id="@+id/imageCameraOne"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_camera"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_blue_200_radius_12_18"
                    android:drawableEnd="@drawable/img_iconplus"
                    android:drawablePadding="@dimen/_28pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_add_product"
                    android:id="@+id/etGroupEightyFive"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_weight="0.48"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_18pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@={addproductVM.addproductModel.etGroupEightyFiveValue}"
                    android:textColorHint="@color/gray_900"
                    style="@style/etSolidRounded_5" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
