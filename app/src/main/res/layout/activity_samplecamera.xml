<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="samplecameraVM" type="com.bedirhansapplication.app.modules.samplecamera.data.viewmodel.SamplecameraVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackpatlamsmsr"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imagePatlamsmsr"
            android:layout_gravity="bottom|start"
            android:layout_height="@dimen/_754pxv"
            android:layout_marginBottom="@dimen/_25pxv"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_patlamsmsr}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_patlamsmsr"/>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleblue_200">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginBottom="@dimen/_23pxv"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCamera"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_156pxh"
                        android:layout_marginStart="@dimen/_110pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.81"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{samplecameraVM.samplecameraModel.txtCamera}"
                        style="@style/txtPoppinssemibold23" tools:text="@string/lbl_camera"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:id="@+id/linearAndroidSmall"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_504pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowclose"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal" style="@style/groupStylebluegray_1">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_red_a700_radius_19_29"
                        android:id="@+id/btnClose"
                        android:layout_height="@dimen/_38pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_38pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_close" tools:ignore="ContentDescription"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_green_a701_radius_19_29"
                        android:id="@+id/btnCheckmark"
                        android:layout_height="@dimen/_38pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_38pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_38pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_checkmark" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <ImageView android:gravity="start"
                    android:id="@+id/imageRectangleFortySix"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_186pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle46"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle46"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
